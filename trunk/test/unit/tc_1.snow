#|
1. Control Flow:
    IF
    IF-ELSE
    IF-ELSEIF
    IF-ELSIF
    IF-ELSEIF-ELSE
    IF-ELSIF-ELSE
|#

after termination:
	print: "Hello Word\n"
	var i = 1
	if i = 1 then
		print: "If Construct Works\n"
	end
	
	if i = 1 then
		print: "If-Else Construct Works\n"
	else
		print: "If-Else Construct Works\n"
	end
	
	if i = 1 then
		print: "If-Elseif Construct Works\n"
	elseif i = 3 then
		print: "If-Elseif Construct Works\n"
	end
	
	if i = 1 then
		print: "If-Elsif Construct Works\n"
	elsif i = 4 then
		print: "If-Elsif Construct Works\n"
	end

	if i = 1 then
		print: "If-Elseif-Else Construct Works\n"
	elseif i = 5 then
		print: "If-Elseif-Else Construct Works\n"
	else
		print: "If-Elseif-Else Construct Works\n"
	end

	if i = 1 then
		print: "If-Elsif-Else Construct Works\n"
	elsif i=6 then
		print: "If-Elsif-Else Construct Works\n"
	else
		print: "If-Elsif-Else Construct Works\n"
	end
end

